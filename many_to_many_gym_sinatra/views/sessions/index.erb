<h1>Session Schedule</h1>
<a href="/sessions/new">Add Course to Schedule</a>
<hr>
<% @sessions.each do |session| %>
  <p><span class="detail-title"> <%= session.course.type %></span> - <%= session.start_time %> </p>
  <p>Session Capacity:  <%= session.capacity %></p>
  <p>Members Registered:  <%= session.members.count %></p>
  <p>Available Spaces:  <%= session.vacancies() %></p>
  <p><a href="/sessions/<%= session.id %>">Session Details</a> </p>
  <hr>
<% end %>
